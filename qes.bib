
@article{conley_supernova_2010,
	title = {{SUPERNOVA} {CONSTRAINTS} {AND} {SYSTEMATIC} {UNCERTAINTIES} {FROM} {THE} {FIRST} {THREE} {YEARS} {OF} {THE} {SUPERNOVA} {LEGACY} {SURVEY}},
	volume = {192},
	issn = {0067-0049},
	url = {https://doi.org/10.1088%2F0067-0049%2F192%2F1%2F1},
	doi = {10.1088/0067-0049/192/1/1},
	abstract = {We combine high-redshift Type Ia supernovae from the first three years of the Supernova Legacy Survey (SNLS) with other supernova (SN) samples, primarily at lower redshifts, to form a high-quality joint sample of 472 SNe (123 low-z, 93 SDSS, 242 SNLS, and 14 Hubble Space Telescope). SN data alone require cosmic acceleration at {\textgreater}99.999\% confidence, including systematic effects. For the dark energy equation of state parameter (assumed constant out to at least z = 1.4) in a flat universe, we find w = –0.91+0.16 –0.20(stat)+0.07 –0.14(sys) from SNe only, consistent with a cosmological constant. Our fits include a correction for the recently discovered relationship between host-galaxy mass and SN absolute brightness. We pay particular attention to systematic uncertainties, characterizing them using a systematic covariance matrix that incorporates the redshift dependence of these effects, as well as the shape-luminosity and color-luminosity relationships. Unlike previous work, we include the effects of systematic terms on the empirical light-curve models. The total systematic uncertainty is dominated by calibration terms. We describe how the systematic uncertainties can be reduced with soon to be available improved nearby and intermediate-redshift samples, particularly those calibrated onto USNO/SDSS-like systems.},
	language = {en},
	number = {1},
	urldate = {2020-05-30},
	journal = {The Astrophysical Journal Supplement Series},
	author = {Conley, A. and Guy, J. and Sullivan, M. and Regnault, N. and Astier, P. and Balland, C. and Basa, S. and Carlberg, R. G. and Fouchez, D. and Hardin, D. and Hook, I. M. and Howell, D. A. and Pain, R. and Palanque-Delabrouille, N. and Perrett, K. M. and Pritchet, C. J. and Rich, J. and Ruhlmann-Kleider, V. and Balam, D. and Baumont, S. and Ellis, R. S. and Fabbro, S. and Fakhouri, H. K. and Fourmanoit, N. and González-Gaitán, S. and Graham, M. L. and Hudson, M. J. and Hsiao, E. and Kronborg, T. and Lidman, C. and Mourao, A. M. and Neill, J. D. and Perlmutter, S. and Ripoche, P. and Suzuki, N. and Walker, E. S.},
	month = dec,
	year = {2010},
	note = {Publisher: IOP Publishing},
	pages = {1},
	file = {IOP Full Text PDF:C\:\\Users\\DonaldAirey\\Zotero\\storage\\HNYBFKQS\\Conley et al. - 2010 - SUPERNOVA CONSTRAINTS AND SYSTEMATIC UNCERTAINTIES.pdf:application/pdf}
}

@article{rodney_type_2012,
	title = {A {TYPE} {Ia} {SUPERNOVA} {AT} {REDSHIFT} 1.55 {INHUBBLE} {SPACE} ℡{ESCOPEINFRARED} {OBSERVATIONS} {FROM} {CANDELS}},
	volume = {746},
	issn = {0004-637X},
	url = {https://doi.org/10.1088%2F0004-637x%2F746%2F1%2F5},
	doi = {10.1088/0004-637X/746/1/5},
	abstract = {We report the discovery of a Type Ia supernova (SN Ia) at redshift z = 1.55 with the infrared detector of the Wide Field Camera 3 (WFC3-IR) on the Hubble Space Telescope (HST). This object was discovered in CANDELS imaging data of the Hubble Ultra Deep Field and followed as part of the CANDELS+CLASH Supernova project, comprising the SN search components from those two HST multi-cycle treasury programs. This is the highest redshift SN Ia with direct spectroscopic evidence for classification. It is also the first SN Ia at z {\textgreater} 1 found and followed in the infrared, providing a full light curve in rest-frame optical bands. The classification and redshift are securely defined from a combination of multi-band and multi-epoch photometry of the SN, ground-based spectroscopy of the host galaxy, and WFC3-IR grism spectroscopy of both the SN and host. This object is the first of a projected sample at z {\textgreater} 1.5 that will be discovered by the CANDELS and CLASH programs. The full CANDELS+CLASH SN Ia sample will enable unique tests for evolutionary effects that could arise due to differences in SN Ia progenitor systems as a function of redshift. This high-z sample will also allow measurement of the SN Ia rate out to z ≈ 2, providing a complementary constraint on SN Ia progenitor models.},
	language = {en},
	number = {1},
	urldate = {2020-05-30},
	journal = {The Astrophysical Journal},
	author = {Rodney, Steven A. and Riess, Adam G. and Dahlen, Tomas and Strolger, Louis-Gregory and Ferguson, Henry C. and Hjorth, Jens and Frederiksen, Teddy F. and Weiner, Benjamin J. and Mobasher, Bahram and Casertano, Stefano and Jones, David O. and Challis, Peter and Faber, S. M. and Filippenko, Alexei V. and Garnavich, Peter and Graur, Or and Grogin, Norman A. and Hayden, Brian and Jha, Saurabh W. and Kirshner, Robert P. and Kocevski, Dale and Koekemoer, Anton and McCully, Curtis and Patel, Brandon and Rajan, Abhijith and Scarlata, Claudia},
	month = jan,
	year = {2012},
	note = {Publisher: IOP Publishing},
	pages = {5},
	file = {IOP Full Text PDF:C\:\\Users\\DonaldAirey\\Zotero\\storage\\D33Q3E8F\\Rodney et al. - 2012 - A TYPE Ia SUPERNOVA AT REDSHIFT 1.55 INHUBBLE SPAC.pdf:application/pdf}
}

@article{rodney_two_2016,
	title = {Two {Type} {Ia} {Supernovae} at {Redshift} {\textasciitilde}2 : {Improved} {Classification} and {Redshift} {Determination} with {Medium}-band {Infrared} {Imaging}},
	volume = {151},
	issn = {1538-3881},
	shorttitle = {Two {Type} {Ia} {Supernovae} at {Redshift} {\textasciitilde}2},
	url = {http://arxiv.org/abs/1508.03100},
	doi = {10.3847/0004-6256/151/2/47},
	abstract = {We present two supernovae (SNe) discovered with the Hubble Space Telescope (HST) in the Cosmic Assembly Near-infrared Deep Extragalactic Legacy Survey (CANDELS), an HST multi-cycle treasury program. We classify both objects as Type Ia SNe and find redshifts of z = 1.80+-0.02 and 2.26 +0.02 -0.10, the latter of which is the highest redshift Type Ia SN yet seen. Using light curve fitting we determine luminosity distances and find that both objects are consistent with a standard Lambda-CDM cosmological model. These SNe were observed using the HST Wide Field Camera 3 infrared detector (WFC3-IR), with imaging in both wide- and medium-band filters. We demonstrate that the classification and redshift estimates are significantly improved by the inclusion of single-epoch medium-band observations. This medium-band imaging approximates a very low resolution spectrum (lambda/delta lambda {\textasciitilde} 100) which can isolate broad spectral absorption features that differentiate Type Ia SNe from their most common core collapse cousins. This medium-band method is also insensitive to dust extinction and (unlike grism spectroscopy) it is not affected by contamination from the SN host galaxy or other nearby sources. As such, it can provide a more efficient - though less precise - alternative to IR spectroscopy for high-z SNe.},
	number = {2},
	urldate = {2020-05-30},
	journal = {The Astronomical Journal},
	author = {Rodney, Steven A. and Riess, Adam G. and Scolnic, Daniel M. and Jones, David O. and Hemmati, Shoubaneh and Molino, Alberto and McCully, Curtis and Mobasher, Bahram and Strolger, Louis-Gregory and Graur, Or and Hayden, Brian and Casertano, Stefano},
	month = feb,
	year = {2016},
	note = {arXiv: 1508.03100},
	keywords = {Astrophysics - Astrophysics of Galaxies, Astrophysics - Cosmology and Nongalactic Astrophysics},
	pages = {47},
	file = {arXiv Fulltext PDF:C\:\\Users\\DonaldAirey\\Zotero\\storage\\54ZQMDU9\\Rodney et al. - 2016 - Two Type Ia Supernovae at Redshift ~2  Improved C.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\DonaldAirey\\Zotero\\storage\\GYHLAKUS\\1508.html:text/html}
}

